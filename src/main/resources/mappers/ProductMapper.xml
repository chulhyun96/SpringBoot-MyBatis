<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.Project1.repository">

    <select id="findAll" resultType="Product">
        SELECT
        *
        FROM product
    </select>

    <insert id="reg" parameterType="Product">
        insert into product(name,selling_price, supplying_price, description ,img)
        values (#{name}, #{sellingPrice}, #{supplyingPrice}, #{description}, #{img})
    </insert>
</mapper>